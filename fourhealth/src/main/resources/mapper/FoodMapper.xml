<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fourhealth.mapper.FoodMapper">
  <insert id="insertFood2" parameterType="java.util.List">
     INSERT INTO tb_food_test
      (foodCode, foodName, foodCal) 
    VALUES
    <foreach collection="list" item="food" separator=" , " >
        ("${food.foodCode}", "${food.foodName}", "${food.foodCal}")
    </foreach>
</insert>
</mapper>